{
  "name": "@bitarray/es6",
  "version": "0.1.0",
  "description": "An ES6 BitArray class for easy operations on sequences of bits",
  "main": "dist/bitarray.js",
  "types": "dist/bitarray.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "build": "tsc -t ES6 --moduleResolution node --declaration --outDir dist/ ./src/bitarray.ts",
    "example": "node --loader ts-node/esm example",
    "test": "node --loader ts-node/esm test/index.ts"
  },
  "author": "swiing",
  "license": "ISC",
  "dependencies": {
    "@bitarray/typedarray": "^1.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.7.0"
  }
}
